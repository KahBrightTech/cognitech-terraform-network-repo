{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowGetAndPutSecret",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:GetSecretValue",
        "secretsmanager:PutSecretValue"
      ],
      "Resource": "arn:aws:secretsmanager:[[region]]:[[account_number]]:secret:int-preproduction-use1-*"
    }
  ]
}
