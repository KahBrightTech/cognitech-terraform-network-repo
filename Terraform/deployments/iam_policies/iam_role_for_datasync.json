{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:GetBucketLocation",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads",
        "s3:ListObjectsV2"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::int-preproduction-use1-shared-services-datasync-bucket",
      "Condition": {
        "StringEquals": {
          "aws:ResourceAccount": "[[account_number]]"
        }
      }
    },
    {
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:GetObjectTagging",
        "s3:GetObjectVersion",
        "s3:GetObjectVersionTagging",
        "s3:ListMultipartUploadParts",
        "s3:PutObject",
        "s3:PutObjectTagging"
      ],
      "Effect": "Allow",
      "Condition": {
        "StringEquals": {
          "aws:ResourceAccount": "[[account_number]]"
        }
      },
      "Resource": [
        "arn:aws:s3:::int-preproduction-use1-shared-services-datasync-bucket",
        "arn:aws:s3:::int-preproduction-use1-shared-services-datasync-bucket/*"
      ]
    }
  ]
}
