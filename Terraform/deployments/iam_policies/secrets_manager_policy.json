{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowGetAndPutSecret",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "secretsmanager:GetSecretValue",
        "secretsmanager:PutSecretValue"
      ],
      "Resource": "*"
    }
  ]
}
