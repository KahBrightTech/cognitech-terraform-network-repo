{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Allow s3 access to admin role only",
      "Principal": {
        "AWS": "*"
      },
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "int-preproduction-use1-shared-config-bucket",
        "int-preproduction-use1-shared-config-bucket/*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:RequestedRegion": ["us-east-1", "us-west-2"]
        }
      }
    }
  ]
}
