{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Allow s3 access to admin role only",
      "Principal": {
        "AWS": "*"
      },
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:PutBucketPolicy"
      ],
      "Resource": ["[[bucket_arn]]", "[[bucket_arn]]/*"]
    }
  ]
}
